audio/*;                   /usr/bin/firefox %s
image/*;                   /usr/bin/firefox %s
application/msword;        /usr/bin/muttopen %s
application/pdf;           /usr/bin/firefox %s
application/postscript;    /usr/bin/muttopen %s
application/json;          /usr/bin/python -m json.tool %s
text/html;                 lynx %s; nametemplate=%s.html
text/html;                 lynx -dump %s; nametemplate=%s.html; copiousoutput
application/*;             /usr/bin/muttopen %s
